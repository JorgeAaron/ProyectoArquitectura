@{
    ViewBag.Title = "Home Page";
}
 </div>

<div class="row ml-0 mr-0">
    <div class="col-md-6 pr-0">
        <div class="card">
            <img class="card-img" src="~/Content/images/adan.gordito.jpg" alt="">
            <div class="card-img-overlay d-flex align-items-center justify-content-center flex-column">
                <p style="color: azure">Aventura</p>
                <hr />
                <h2 style="color: azure" >
                    ¿Quieres recorrer san carlos?
                </h2>
            </div>
        </div>
    </div>
    <div class="col-md-6 pl-0">
        <div class="card">
            <img class="card-img" src="~/Content/images/racers.jpg" alt="">
            <div class="card-img-overlay d-flex align-items-center justify-content-center flex-column">
                <p style="color: azure">Escoge la tuya</p>
                <hr />
                <h2 style="color: azure">
                    ¿Cual prefieres?
                </h2>
            </div>
        </div>
    </div>

    <div class="col-md-3 pr-0 first">
        <a href="article.html">
            <div class="card">
                <img class="card-img" src="~/Content/images/motos.jpg" alt="">
                <div class="card-img-overlay">
                    <h5 style="color: azure">Motos (tours)</h5>
                </div>
            </div> <a />
    </div>

    <div class="col-md-3 pl-0 pr-0">
        <a href="@Url.Action("Tienda", "Home")">
            <div class="card" >
                <img class="card-img" src="~/Content/images/tienda.jpg" alt="">
                <div class="card-img-overlay" style="object-position:center">
                    <h5 style="color: azure">Tienda</h5>
                </div>
            </div>
        </a>
    </div>

    <div class="col-md-3 pl-0 pr-0">
        <a href="article.html">
            <div class="card">
                <img class="card-img" src="~/Content/images/car.wahs.jpg" alt="">
                <div class="card-img-overlay">
                    <h5 style="color: azure">Car wash</h5>
                </div>
            </div>
        </a>
    </div>


    <div class="col-md-3 pl-0 last">
        <a href="@Url.Action("Otros", "Home")">
            <div class="card">
                <img class="card-img" src="~/Content/images/kayak.jpg" alt="">
                <div class="card-img-overlay">
                    <h5 style="color: azure">Otgggros</h5>
                </div>
            </div> <a/>
    </div>

</div>

<div class="container-fluid video-player">
    <div class="container">
        <div class="screen embed-responsive embed-responsive-16by9">
            <iframe id="screen" width="560" height="315" src="https://www.youtube.com/embed/KK_jqlABOuI"
                    frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen>
                </>
            </iframe>
        </div>


        <hr>


    </div>
</div>



